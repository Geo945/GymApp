<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="588.0" prefWidth="870.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="proiect.fis.gym.aplication.controllers.GymManagerProfileController">
   <children>

      <ImageView fx:id="profilePicImageView" fitHeight="150.0" fitWidth="200.0" layoutX="43.0" layoutY="40.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../gym_default_profile_pic.jpg" />
         </image></ImageView>
      <Label fx:id="gymNameLabel" layoutX="330.0" layoutY="39.0" prefHeight="26.0" prefWidth="268.0" text="gym name">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="managerNameLabel" layoutX="330.0" layoutY="65.0" prefHeight="26.0" prefWidth="293.0" text="Name">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label id="locationLabel" fx:id="locationLabel" layoutX="330.0" layoutY="92.0" prefHeight="26.0" prefWidth="345.0" text="Location">
         <font>
            <Font size="15.0" />
         </font></Label>

      <Button id="addCourseButton" layoutX="63.0" layoutY="221.0" mnemonicParsing="false" onAction="#handleAddCourseButton" prefHeight="26.0" prefWidth="160.0" text="Add course" />

      <Label fx:id="emailLabel" layoutX="330.0" layoutY="118.0" prefHeight="26.0" prefWidth="346.0" text="email">
         <font>
            <Font size="15.0" />
         </font></Label>
      <Label fx:id="phoneLabel" layoutX="330.0" layoutY="145.0" prefHeight="26.0" prefWidth="346.0" text="tel">
         <font>
            <Font size="15.0" />
         </font></Label>
      <ImageView fitHeight="19.0" fitWidth="19.0" layoutX="297.0" layoutY="149.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons8-phone-50.png" />
         </image></ImageView>
      <ImageView fitHeight="19.0" fitWidth="19.0" layoutX="297.0" layoutY="122.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons8-email-64.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="19.0" fitWidth="19.0" layoutX="297.0" layoutY="96.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons8-marker-50.png" />
         </image>
      </ImageView>

      <GridPane fx:id="courseGridPane" layoutX="300.0" layoutY="270.0" prefHeight="91.0" prefWidth="450" visible="false">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="115.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="335.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="30.4" minHeight="10.0" prefHeight="30.4" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="32.4" minHeight="10.0" prefHeight="30.4" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label prefHeight="30.0" prefWidth="124.0" text="Nume curs:">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Label prefHeight="30.0" prefWidth="115.0" text="Nume antrenor:" GridPane.rowIndex="1">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Label text="Program: " GridPane.rowIndex="2">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <TextField fx:id="courseNameTextField" prefHeight="31.0" prefWidth="204.0" text="curs" GridPane.columnIndex="1">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
            <TextField fx:id="trainerNameTextField" text="trainer" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
            <TextField fx:id="scheduleTextField" text="program" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
         </children>
      </GridPane>

      <Button id="logOutButton" layoutX="698.0" layoutY="40.0" mnemonicParsing="false" onAction="#handleLogOutButton" text="Log out" />

      <Button fx:id="submitNewCourseButton" layoutX="599.0" layoutY="371.0" mnemonicParsing="false" onAction="#handleSubmitNewCourseButton" prefHeight="26.0" prefWidth="150.0" text="Submit new course" visible="false" />

      <Button layoutX="63.0" layoutY="270.0" mnemonicParsing="false" onAction="#handleViewCoursesButton" prefHeight="26.0" prefWidth="160.0" text="View Courses" />

      <Label fx:id="errorMessageAddCourseLabel" alignment="CENTER_RIGHT" layoutX="555.0" layoutY="429.0" prefHeight="26.0" prefWidth="200" text="trm" visible="false">
         <font>
            <Font size="15.0" />
         </font>
      </Label>

      <Button layoutX="63.0" layoutY="316.0" mnemonicParsing="false" onAction="#handleViewCustomersButton" prefHeight="26.0" prefWidth="160.0" text="View Customers" />

      <ImageView fitHeight="22.0" fitWidth="26.0" layoutX="296.0" layoutY="42.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../gym-icon.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="26.0" fitWidth="29.0" layoutX="293.0" layoutY="65.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons8-person-64.png" />
         </image>
      </ImageView>

      <Button layoutX="63.0" layoutY="358.0" mnemonicParsing="false" onAction="#handleAddProfilePhotoButton" prefHeight="26.0" prefWidth="160.0" text="Add profile photo" />

